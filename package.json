  "description": "Processes thousands of posts with parallel encoding, incremental builds, and atomic writes!",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "bin": {
    "odor": "./bin/odor.js",
    "odor-complaint": "./bin/odor-complaint.js",
    "odor-agent": "./bin/odor-agent.js"
  },
  "exports": {
    ".": "./src/lib/index.js",
    "./queue": "./src/queue/index.js",
    "./kit": "./src/kit/index.js"
  },
  "files": [
